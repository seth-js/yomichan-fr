{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
        "type",
        "audioSources"
    ],
    "additionalProperties": false,
    "properties": {
        "type": {
            "type": "string",
            "const": "audioSourceList"
        },
        "audioSources": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "url"
                ],
                "additionalProperties": false,
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "url": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
